{
    "name": "opendesa/e-sign-bsre",
    "description": "Package E-Sign BSrE untuk memudahkan penggunaan E-Sign BSrE (Balai Sertifikasi Elektronik) API dari BSSN (Badan Sandi dan Siber Negara)",
    "keywords": [
        "e-sign-bsre"
    ],
    "homepage": "https://github.com/OpenSID/E-Sign-BSrE",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andi Fahruddin Akas",
            "email": "andifahruddinakas@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "OpenDesa\\ESignBsre\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenDesa\\ESignBsre\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
